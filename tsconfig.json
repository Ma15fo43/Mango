{
	"compilerOptions": {
		"target": "esnext",
		"module": "commonjs",
		"lib": [
			"es2015",
			"es2016",
			"es2017",
			"esnext"
		],
		"allowJs": false,
		"checkJs": false,
		"declaration": false,
		"declarationMap": false,
		"sourceMap": false,
		// "outDir": "./out",
		// "rootDir": "./src",
		"removeComments": false,
		"strict": false,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"typeRoots": [
			"./api/typings",
			"./node_modules/@types"
		]
	},
	"exclude": [
		"node_modules"
	],
}

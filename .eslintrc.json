{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "extends": "eslint:recommended",
    "rules": {
        "no-undef": "off",
        "prefer-const": "error",
        "eol-last": "warn"
    }
}